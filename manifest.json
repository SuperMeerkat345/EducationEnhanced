{
    "manifest_version": 3,
    "name": "Education Enhanced",
    "description": "Collection of tools which help with productivity",
    "version": "0.0.1",
    "action": {
      "default_popup": "popup.html"
    },
    "permissions": [
      "tabs",
      "activeTab"
    ],
    "content_scripts": [
      {
        "js": ["script.js"],
        "matches": [
          "https://*/*",
          "http://*/*"
        ]
      }
    ],
    "icons": {
      "16": "images/icon-16.png",
      "32": "images/icon-32.png",
      "48": "images/icon-48.png",
      "128": "images/icon-128.png"
    }
}


  